{
    "requireRemap": [
        {
            "from": "@ebay/ebayui-core/dist/components/ebay-carousel/style-no-touch.less",
            "to": "@ebay/ebayui-core/dist/components/ebay-carousel/style-touch.less",
            "if-flag": "mobile"
        }
    ],
    "dependencies": [
        "require-run: jquery",
        "site-speed-ebay/optimizer.json",
        {
            "type": "require",
            "path": "cookies-browser",
            "run": true
        },
        "ebay-font/browser.json",
        "./vq-font.less",
        "./common.less",
        "@ebay/skin/less",
        "@ebay/skin/global",
        "@ebay/skin/spinner",
        "@ebay/skin/checkbox",
        "@ebay/skin/utility",
        "@ebay/skin/button",
        {
            "if-not-flag": "mobile",
            "dependencies": [
                "./ds6.override.less"
            ]
        },
        {
            "if-flag": "mobile",
            "dependencies": [
                "./style[mobile].less",
                "./ds6.override[mobile].less"
            ]
        },
        {
            "if-flag": "polyfill",
            "dependencies": [
                    "require-run: core-js/features/promise",
                    "require-run: whatwg-fetch",
                    "require-run: core-js/features/array/from",
                    "require-run: core-js/features/array/includes",
                    "require-run: core-js/features/object/assign"
            ]
        }
    ]
}
