{
    "ssl": {
        "key": "file:./cert/server.key",
        "cert": "file:./cert/server.crt"
    },
    "lasso": {
        "require": {
            "transforms": [
                {
                    "transform": "lasso-babel-transform",
                    "config": {
                        "extensions": [
                            ".js",
                            ".es6",
                            ".marko"
                        ],
                        "babelOptions": {
                            "babelrc": false
                        }
                    }
                }
            ]
        },
        "plugins": [
            "i18n-ebay/optimizer/plugin",
            "lasso-less",
            {
                "plugin": "lasso-marko",
                "config": {
                    "useCache": true
                }
            },
            "arc-lasso"
        ],
        "url-prefix": "/static",
        "outputDir": "static",
        "fingerprintsEnabled": false,
        "minify": false,
        "bundlingEnabled": false,
        "resolveCssUrls": true,
        "cacheProfile": "development"
    },
