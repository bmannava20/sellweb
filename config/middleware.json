{
    "security-ebay": {
        "enabled": true,
        "priority": 2,
        "module": {
            "name": "security-ebay/middleware"
        }
    },
    "static": {
        "enabled": true,
        "priority": 20,
        "route": "/static",
        "module": {
            "name": "serve-static",
            "arguments": [
                "static"
            ]
        }
    },
    "favicon": {
        "enabled": false
    },
    "compress": {
        "enabled": true
    },
    "meta-router/match": {
        "enabled": true,
        "priority": 79,
        "module": {
            "name": "meta-router/middleware/match",
            "arguments": [
                "path:./routes.json"
            ]
        }
    },
    "logger": {
        "module": {
            "name": "morgan",
            "arguments": [
                "short"
            ]
        }
    },
    "requestlocal": {
        "enabled": true,
        "priority": 80,
        "module": {
            "name": "request-local/middleware",
            "method": "create"
        }
    },
    "cal": {
        "enabled": true,
        "priority": 87,
        "module": {
            "name": "cal/middleware",
            "arguments": [
                {
                    "enableNestedCal": true,
                    "bufferFlushDelay": 500
                }
            ]
        }
    },
    "monitor-inc": {
        "enabled": true,
        "priority": 91,
        "module": {
            "name": "monitor-inc",
            "method": "init"
        }
    },
    "mirror": {
        "enabled": true,
        "priority": 101,
        "module": "traffic-mirror-ebay"
    },
    "commonsebay": {
        "enabled": true,
        "priority": 102,
        "module": {
            "name": "commons-ebay/middleware",
            "arguments": [
                {
                    "cal": {
                        "type": "cal",
                        "formatter": "cal"
                    }
                }
            ]
        }
    },
    "httptohttpsebay": {
        "enabled": true,
        "priority": 103,
        "module": {
            "name": "http-to-https-ebay",
            "arguments": [
                {
                    "excludePattern": "warmup=true"
                }
            ]
        }
    },
    "cookies": {
        "enabled": true,
        "priority": 111,
        "module": "cookies-ebay/middleware"
    },
    "oauth": {
        "enabled": true,
        "priority": 112,
        "module": "oauth-ebay"
    },
    "locale": {
        "enabled": true,
        "priority": 113,
        "module": "locale-ebay"
    },
    "tracking": {
        "enabled": true,
        "priority": 114,
        "module": {
            "name": "tracking-ebay",
            "method": "middleware"
        }
    },
    "experimentation": {
        "enabled": true,
        "priority": 115,
        "module": "experimentation-ebay/middleware"
    },
    "rate-limiter": {
        "enabled": true,
        "priority": 116,
        "module": {
            "name": "ratelimiter-ebay/middleware"
        }
    },
    "ebay-i18n": {
        "enabled": true,
        "priority": 117,
        "module": {
            "name": "i18n-ebay/middleware",
            "arguments": [
                {}
            ]
        }
    },
    "pds": {
        "enabled": true,
        "priority": 118,
        "module": "pds-ebay/middleware"
    },
    "device-detection": {
        "enabled": true,
        "priority": 119,
        "module": {
            "name": "device-detection-ebay",
            "method": "middleware"
        }
    },
    "global-header": {
        "enabled": true,
        "priority": 120,
        "module": "global-header-ebay/middleware"
    },
    "site-speed": {
        "enabled": true,
        "priority": 121,
        "module": "site-speed-ebay"
    },
    "auth-for-ajax": {
        "enabled": true,
        "priority": 122,
        "module": {
            "name": "path:../src/lib/middleware/auth-for-ajax/"
        }
    },
    "auth": {
        "enabled": true,
        "priority": 123,
        "fallback": {
            "warmup": {
                "enabled": true,
                "priority": 10,
                "module": {
                    "name": "path:../src/lib/middleware/auth-for-warmup",
                    "method": "warmupByPassAuth"
                }
            },
            "auth-ebay": {
                "enabled": true,
                "priority": 20,
                "module": {
                    "name": "auth-ebay/middleware",
                    "method": "auth"
                }
            }
        }
    },
    "csrf_ebay": {
        "enabled": true,
        "priority": 124,
        "module": {
            "name": "csrf-ebay/middleware",
            "arguments": [
                {
                    "maxAge": "config:maxAge",
                    "GET_enforcement": "NONE",
                    "invalidHandler": "path:../src/lib/middleware/csrf-error",
                    "enforcement": "AUTH"
                }
            ]
        }
    },
    "adaptive": {
        "enabled": true,
        "priority": 126,
        "module": {
            "name": "path:../src/lib/middleware/adaptive"
        }
    },
    "pollyfill": {
        "enabled": true,
        "priority": 127,
        "module": {
            "name": "path:../src/lib/middleware/pollyfill"
        }
    },
    "multiact-perm-enf": {
        "enabled": true,
        "priority": 128,
        "module": {
          "name": "multiact-perm-enf"
        }
    },
    "browser-upgrade": {
        "enabled": true,
        "priority": 129,
        "module": {
            "name": "path:../src/lib/middleware/browser-upgrade"
        }
    },
    "meta-router/invokeHandler": {
        "enabled": true,
        "priority": 1000,
        "module": {
            "name": "meta-router/middleware/invokeHandler",
            "arguments": []
        }
    },
    "serverError500": {
        "enabled": true,
        "priority": 1001,
        "module": {
            "name": "path:../src/lib/middleware/errors/server500"
        }
    },
    "pnf404": {
        "enabled": true,
        "priority": 1002,
        "module": "brogan-ebay/middleware/404"
    }
}
